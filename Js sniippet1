$(function() {
	$("#datepicker").datepicker({
		autoclose: true,
		todayHighlight: true,
		format: 'dd/mm/yyyy',
		endDate: '+0d',
		autoclose: true,
		startDate: '01-01-1920'
	}).datepicker('update', new Date());
});



